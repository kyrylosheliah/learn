{
	"version": "2.0.0",
	"command": "dotnet",
	"tasks": [
		{
			"label": "build-problem",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/Problem/Problem.csproj",
				"-c", "Debug",
			],
			"problemMatcher": "$msCompile",
			"group": {
				"kind": "build",
				"isDefault": true,
			},
		}
	]
}